  resnet50_wasm
  |> Wasmtube.call_function("predict", image: kino_image.content, width: 224, height: 224)
end

mobilenetv2_wasm_func = fn ->
  mobilenetv2_wasm
  |> Wasmtube.call_function("predict", image: kino_image.content, width: 224, height: 224)
end

Benchee.run(%{
  "Wasm (ResNet50)" => resnet50_wasm_func,
  "Wasm (MobileNetV2)" => mobilenetv2_wasm_func
})
```

